<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="435.0" prefWidth="766.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DisplayLeasesController">
   <children>
      <Label layoutX="25.0" layoutY="65.0" prefHeight="17.0" prefWidth="242.0" text="Display Leases">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="leaseTable" layoutX="11.0" layoutY="130.0" prefHeight="239.0" prefWidth="744.0">
         <columns>
            <TableColumn fx:id="name" prefWidth="96.0" text="NAME" />
            <TableColumn fx:id="email" prefWidth="131.0" text="EMAIL ID" />
            <TableColumn fx:id="address" prefWidth="137.0" text="ADDRESS" />
            <TableColumn fx:id="startDate" prefWidth="162.0" text="START DATE" />
            <TableColumn fx:id="endDate" prefWidth="140.0" text="END DATE" />
            <TableColumn fx:id="rent" prefWidth="77.0" text="RENT" />
         </columns>
      </TableView>
      <Label layoutX="25.0" layoutY="104.0" prefHeight="17.0" prefWidth="98.0" text="Leases List" underline="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="buttonRefresh" layoutX="666.0" layoutY="100.0" mnemonicParsing="false" onAction="#refreshList" prefHeight="25.0" prefWidth="83.0" text="Refresh" />
   </children>
</AnchorPane>
