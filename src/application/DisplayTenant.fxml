<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.DisplayTenantController">
   <children>
      <Label layoutX="26.0" layoutY="55.0" prefHeight="17.0" prefWidth="242.0" text="Display Tenants">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="tenantTable" layoutX="12.0" layoutY="120.0" prefHeight="239.0" prefWidth="744.0">
         <columns>
            <TableColumn fx:id="name" prefWidth="287.0" text="NAME" />
            <TableColumn fx:id="email" prefWidth="452.0" text="EMAIL ID" />
         </columns>
      </TableView>
      <Label layoutX="26.0" layoutY="94.0" prefHeight="17.0" prefWidth="98.0" text="Tenants List" underline="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="buttonRefresh" layoutX="667.0" layoutY="90.0" mnemonicParsing="false" onAction="#refreshList" prefHeight="25.0" prefWidth="83.0" text="Refresh" />
   </children>
</AnchorPane>
